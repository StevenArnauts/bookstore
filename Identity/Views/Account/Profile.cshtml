@model Bookstore.Identity.Controllers.ProfileModel
@{
	ViewData["Title"] = "Profile";
	Layout = "Shell";
}
<h2>Profile</h2>
<table class="table">
	<thead>
		<tr>
			<th>Name</th>
			<th>Value</th>
		</tr>
	</thead>
	<tbody>
		@foreach (System.Security.Claims.Claim claim in Model.Claims) {
			<tr>
				<td>@claim.Type</td>
				<td>@claim.Value</td>
			</tr>
		}
	</tbody>
</table>